<h1>Hola! from admin dashboard </h1>

<% @lines.each do |line| %>
  <p><%= line %></p>
<% end %>

<ul>
  <% @users.each do |user| %>
    <li><%= user.company %></li>
  <% end %>
</ul>

<%= link_to 'Ajouter un produit', new_product_path %>
<br>
<br>
<ul>
  <% @products.each do |product| %>
    <li><%= link_to product.name, product_path(product) %></li>
  <% end %>
</ul>
